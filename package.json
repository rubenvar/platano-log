{
  "name": "@platanojs/log",
  "version": "0.1.0",
  "description": "üçå Logs a platano! (no dependencies)",
  "main": "index.js",
  "devDependencies": {
    "@release-it/keep-a-changelog": "^2.3.0",
    "release-it": "^14.11.6"
  },
  "scripts": {
    "release": "release-it"
  },
  "release-it": {
    "git": {
      "commitMessage": "v${version}"
    },
    "github": {
      "release": true
    },
    "plugins": {
      "@release-it/keep-a-changelog": {
        "filename": "CHANGELOG.md"
      }
    }
  },
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rubenvar/platano-log.git"
  },
  "keywords": [
    "banana",
    "platano",
    "console",
    "console-log"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/rubenvar/platano-log/issues"
  },
  "homepage": "https://github.com/rubenvar/platano-log#readme"
}
